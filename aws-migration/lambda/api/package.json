{
  "name": "smuppy-lambda-api",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.975.0",
    "@aws-sdk/client-dynamodb": "^3.975.0",
    "@aws-sdk/client-s3": "^3.975.0",
    "@aws-sdk/client-secrets-manager": "^3.975.0",
    "@aws-sdk/client-sns": "^3.975.0",
    "@aws-sdk/s3-request-presigner": "^3.975.0",
    "firebase-admin": "^12.0.0",
    "google-auth-library": "^9.6.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "pg": "^8.14.1",
    "stripe": "^20.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.147",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/pg": "^8.11.11",
    "@types/uuid": "^9.0.7",
    "esbuild": "^0.27.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.6",
    "typescript": "~5.9.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/__tests__"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "**/*.ts",
      "!**/*.d.ts",
      "!**/__tests__/**",
      "!**/node_modules/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}
