      "environment": "production",
      "env": {
        "APP_ENV": "production",
        "SENTRY_DISABLE_AUTO_UPLOAD": "true"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6757627406"
      }
    }
  }
}
