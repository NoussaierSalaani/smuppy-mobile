{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_ENV": "dev"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "APP_ENV": "dev"
      }
    },
    "production": {
